{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
  },
  "parser": "babel-eslint",
  "plugins": [
    "import"
  ],
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolve": {
      moduleDirectory: ["node_modules", "app"]
    }
  },
  "rules": {
    "camelcase": [2, {"properties": "always"}],
    "comma-dangle": 0,
    "consistent-return": 2,
    "curly": [2, "multi-line"],
    "default-case": 2,
    "eqeqeq": 2,
    "func-names": 1,
    "import/default": 0,
    "import/named": 2,
    "import/namespace": 2,
    "import/no-duplicates": 0,
    "import/no-named-as-default": 0,
    "import/no-unresolved": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "jsx-quotes": 1,
    "keyword-spacing": 2,
    "max-len": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 2,
    "no-shadow": 0,
    "no-trailing-spaces": 0,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": [2, {'vars': 'local', 'args': 'after-used'}],
    "no-use-before-define": 2,
    "no-var": 2,
    "object-curly-spacing": [1, "always"],
    "prefer-const": 2,
    "quotes": [2, "single", "avoid-escape"],
    "semi": [2, "never"],
    "strict": 0,
    "yoda": 2
  },
  "globals": {
    "__DEV__": '',
    "__API_KEY__": '',
    "__API_ROOT__": '',
    "__API_ROOT_SECURE__": '',
    "__SAMPLE_DATA__": '',
    "Promise": '{}'
  }
}